<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
	prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="de.tu_bs.cs.isf.e4cf.parts.project_explorer.controller.NewFileViewController">
	<children>
		<Text strokeType="OUTSIDE" strokeWidth="0.0"
			text="Create a new file" />
		<TextField fx:id="fileNameTextField" promptText="Filename" />
		<Text fx:id="notValidText" fill="#e50808" strokeType="OUTSIDE"
			strokeWidth="0.0" text="Invalid Filename" visible="false" />
		<Text fx:id="alreadyExitsText" fill="#e50808"
			strokeType="OUTSIDE" strokeWidth="0.0" text="File already exists"
			visible="false" />
	</children>
</VBox>
